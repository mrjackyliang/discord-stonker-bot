{
  "env": {
    "node": true
  },
  "extends": [
    "airbnb-base",
    "plugin:import/typescript"
  ],
  "ignorePatterns": [
    "/build/**/*"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "root": true,
  "rules": {
    "import/extensions": [
      "error",
      "ignorePackages"
    ],
    "import/no-default-export": [
      "error"
    ],
    "import/no-unresolved": [
      "off"
    ],
    "import/prefer-default-export": [
      "off"
    ],
    "max-len": [
      "off"
    ],
    "no-console": [
      "off"
    ]
  }
}
